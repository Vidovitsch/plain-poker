{
  "name": "PlainPoker",
  "version": "0.1.0",
  "private": true,
  "author": "Vidovitsch",
  "homepage": "./",
  "build": {
    "appId": "com.example.electron-cra",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "main": "public/main.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"set BROWSER=none&& npm start\" \"wait-on http://localhost:3001 && electron .\"",
    "electron-pack": "build -c.extraMetadata.main=build/main.js",
    "preelectron-pack": "npm run build"
  },
  "dependencies": {
    "dotenv": "^5.0.1",
    "electron": "^1.8.6",
    "electron-builder": "^20.13.4",
    "electron-is-dev": "^0.3.0",
    "plain-poker-gateway": "git://github.com/Vidovitsch/plain-poker-gateway.git",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "uuid": "^3.2.1",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "wait-on": "^2.1.0"
  }
}
